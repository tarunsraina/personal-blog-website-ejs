<%- include header %>

<h1>Home</h1>

<p> <%=rand1%>  <%=rand2%>  <%=rand3%> </p>

<% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <% let postTrim = post.body.substring(0,100) +"..."; %>
    <p><%= postTrim %></p>
    <a href="/posts/<%=post.title%>" >Read more..</a>
<% }); %>



<%- include footer %>